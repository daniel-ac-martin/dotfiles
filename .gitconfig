[user]
	email = 
	name = Daniel A.C. Martin
[url "https://"]
	insteadOf = git://
[core]
	editor = vi
[push]
	default = simple
[alias]
	buttery-biscuit = pull --rebase origin master
	new-branch = "!f() { name=${1}; git checkout master && git pull && git checkout -b ${name} && git push -u origin ${name}; }; f"
	staged = diff --staged
	nb = !git new-branch
	bb = !git buttery-biscuit
